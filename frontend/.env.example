# API Configuration
VITE_API_URL=http://localhost:8000
VITE_API_VERSION=v1

# Application Configuration
VITE_APP_NAME=KingMaker
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION=차세대 퀀트 투자 플랫폼

# Environment
VITE_NODE_ENV=development

# Authentication
VITE_JWT_SECRET=your-jwt-secret-key
VITE_JWT_EXPIRES_IN=7d

# External Services
VITE_SENTRY_DSN=
VITE_GOOGLE_ANALYTICS_ID=
VITE_HOTJAR_ID=

# Feature Flags
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false
VITE_ENABLE_PERFORMANCE_MONITORING=false
VITE_ENABLE_DEBUG_MODE=true

# WebSocket Configuration
VITE_WS_URL=ws://localhost:8000/ws
VITE_WS_RECONNECT_INTERVAL=5000
VITE_WS_MAX_RECONNECT_ATTEMPTS=10

# Market Data Configuration
VITE_MARKET_DATA_PROVIDER=yahoo
VITE_MARKET_DATA_API_KEY=
VITE_MARKET_DATA_REFRESH_INTERVAL=60000

# Chart Configuration
VITE_CHART_DEFAULT_THEME=light
VITE_CHART_ANIMATION_DURATION=300

# Pagination
VITE_DEFAULT_PAGE_SIZE=20
VITE_MAX_PAGE_SIZE=100

# File Upload
VITE_MAX_FILE_SIZE=10485760
VITE_ALLOWED_FILE_TYPES=.csv,.xlsx,.json

# Cache Configuration
VITE_CACHE_TTL=300000
VITE_CACHE_MAX_SIZE=100

# Development
VITE_DEV_SERVER_PORT=3000
VITE_DEV_SERVER_HOST=localhost
VITE_DEV_PROXY_ENABLED=true